<h1>Listing publishers</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publishers.each do |publisher| %>
      <tr>
        <td><%= publisher.name %></td>
        <td><%= link_to 'Show', admin_publisher_path(publisher) %></td>
        <td><%= link_to 'Edit', edit_admin_publisher_path(publisher) %></td>
        <td><%= link_to 'Destroy', admin_publisher_path(publisher), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publisher', new_admin_publisher_path %>
