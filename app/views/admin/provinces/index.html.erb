<h1>Listing provinces</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Tax</th>
      <th>Show</th>
      <th>Edit</th>
      <th>Destroy</th>
    </tr>
  </thead>

  <tbody>
    <% @provinces.each do |province| %>
      <tr>
        <td><%= province.name %></td>
        <td><%= province.tax %>%</td>
        <td><%= link_to 'Show', admin_province_path(province) %></td>
        <td><%= link_to 'Edit', edit_admin_province_path(province) %></td>
        <td><%= link_to 'Destroy', admin_province_path(province), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Province', new_admin_province_path %>
